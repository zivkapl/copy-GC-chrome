{
    "name": "copy-GC",
    "version": "2.0",
    "manifest_version": 3,
    "action": {},
    "icons": { "2": "media/copy-gc-logo.png"},
    "permissions": [
        "activeTab",
        "scripting",
        "clipboardWrite",
        "clipboardRead"
    ],
    "content_scripts": [
        {
            "matches": ["https://guardicore.atlassian.net/jira/software/c/projects/*/boards/*"],
            "js": ["src/dashboard-script.js"],
            "css": ["src/styles.css"]
          }
      ]
}